{
  "name": "@fastpanel/mongodb",
  "version": "0.0.1",
  "description": "Extension to work with \"MongoDB\" base.",
  "author": "Desionlab <fenixphp@gmail.com>",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "develop": "./node_modules/.bin/tsc --watch",
    "build": "./node_modules/.bin/tsc && npm run test && npm run docs",
    "docs": "typedoc --theme minimal --target 'esnext' --excludeNotExported --excludePrivate --ignoreCompilerErrors --out ./docs/ ./src/",
    "test": "node ./node_modules/mocha/bin/mocha --require ts-node/register  test/**/*.spec.ts",
    "prepare": "./node_modules/.bin/tsc && npm run test && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desionlab/fastpanel-extension-mongodb.git"
  },
  "bugs": {
    "url": "https://github.com/desionlab/fastpanel-extension-mongodb/issues"
  },
  "homepage": "https://github.com/desionlab/fastpanel-extension-mongodb#readme",
  "keywords": [
    "nodejs",
    "typescript",
    "mongodb",
    "mongoose"
  ],
  "dependencies": {
    "mongoose": "^5.4.0",
    "mongoose-autopopulate": "^0.9.0",
    "mongoose-bcrypt": "^1.6.0",
    "mongoose-hidden": "^1.6.0",
    "mongoose-mpath": "^2.0.1"
  },
  "devDependencies": {
    "@fastpanel/core": "github:desionlab/fastpanel-core#develop",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.3.5",
    "@types/node": "^10.12.17",
    "@types/progress": "^2.0.3",
    "@types/vorpal": "^1.11.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.2.2"
  },
  "license": "MIT"
}
